<tr>
  <th>Симптомы:</th>
  <td>
    <table class="table">
      <thead>
      <tr>
        <th>Симптом</th>
        <th>Дата постановки симптома</th>
      </tr>
      </thead>
      <tbody>
      <%	@illness.symptoms.each  do |symptom| %>
          <tr>
            <td style="width: 15%"> <%=	symptom.title %></td>
            <td><%=	symptom.created_at %></td>
          </tr>
      <%	end %>
      </tbody>
    </table>
  </td>
</tr>

